mkdir lib
cd lib
wget https://www.cs.princeton.edu/~appel/modern/java/JLex/Archive/1.2.6/Main.java
javac Main.java
jar cvfe JLex.jar Main *.class
rm *.class
rm Main.java
wget -O cup.tar.gz http://www2.cs.tum.edu/projects/cup/download.php?file=java-cup-bin-11b-20160615.tar.gz
tar -xvzf cup.tar.gz
rm cup.tar.gz
